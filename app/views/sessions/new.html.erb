<h1 class="center">Login Page</h1>
<%= flash[:message] if flash[:message]%>

<div class="center">
    <p>
        <%= form_for :user do |f| %>

  
        <%= f.label :username %>
        <%= f.text_field  :username %>
  
        <%= f.label :password %>
        <%= f.password_field :password %>

        <%= f.submit "Login"  %>

        <% end %> 
    </p>
</div>
