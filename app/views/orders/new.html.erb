<h2>Create A New Order</h2>


<%= form_for(@order) do |f| %>

<%= f.label :shipping_type%>
<%= f.text_field :shipping_type%><br>

<%= f.label :shipping_address %>
<%= f.text_field :shipping_address%><br>

<%= f.label :card_type%>
<%= f.text_field :card_type%><br>

<%= f.label :card_number%>
<%= f.text_field :card_number%><br>

<%= f.submit %>

<% end %>