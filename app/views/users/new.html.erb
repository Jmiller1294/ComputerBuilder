<h1 > Signup Page </h1>

<%= render partial: "layouts/errors", locals: {object: @user}%>

<body>
<p>
        <%= form_for(@user, url: "/signup") do |f|%>
        <%=f.label :username %>
        <%=f.text_field :username %>
        <br>
        <%=f.label :email %>
        <%= f.email_field :email%>
        <br>
        <%=f.label :password %>
        <%= f.password_field :password%><br>

        <%= f.submit :Signup %>
        <%end%>
    </p>
</body>